{
  "name": "client",
  "version": "0.1.0",
  "description": "News App",
  "private": true,
  "dependencies": {
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "prettier-eslint": "^9.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "sass-loader": "^8.0.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel webpack-start watch:sass",
    "build": "webpack -p",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass src/styles/sass/main.scss src/styles/css/App.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/App.comp.css",
    "clean": "rm package-lock.json && rm -Rf node_modules",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "webpack-start": "webpack-dev-server --host 0.0.0.0 --hot --progress --colors --inline --port 3000 --open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "autoprefixer": "^9.7.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concat": "^1.0.3",
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.12.18",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
